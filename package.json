{
  "name": "video-storyteller",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prestart": "ts-node -P tsconfig.scripts.json scripts/createPlaceholderImages.ts",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prerender": "react-snapshot",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "cpd": "jscpd src --ignore src/assets/img/*",
    "ci": "cross-env NODE_OPTIONS=--max_old_space_size=4096 CI=true concurrently --kill-others-on-fail --names cpd,lint,test,build --prefix-colors white,cyan,yellow,magenta npm:cpd npm:lint npm:test npm:build",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:hosting:ci": "npm run deploy:hosting -- --token $FIREBASE_TOKEN  --project default",
    "deploy": "firebase deploy",
    "deploy:ci": "npm run deploy -- --token $FIREBASE_TOKEN --project default",
    "serve:build": "http-server build",
    "commitlint": "commitlint --from HEAD~1",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "docs": "typedoc --out docs src",
    "docs:deploy": "surge docs --domain https://video-storyteller-docs.surge.sh",
    "semantic-release": "semantic-release"
  },
